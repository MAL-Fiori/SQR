<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:custom="com.sapZSQRMBWA.control" xmlns="sap.uxap" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:table="sap.ui.table"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" controllerName="com.sapZSQRMBWA.controller.CreateInspection">
	<m:App id="addinspection">
		<m:pages>
			<m:Page title="Title" showNavButton="true" navButtonPress="onNavBack">
				<!--<content>-->
					<ObjectPageLayout enableLazyLoading="false">
					<headerTitle>
						<ObjectPageHeader objectTitle="Inspection"></ObjectPageHeader>
					</headerTitle>
					<headerContent>
						<m:ObjectAttribute title="Inspection ID" text="{ZSQRMBWA>Id}"/>
					</headerContent>
					<sections>
						<ObjectPageSection title="Info">
							<subSections>
								<ObjectPageSubSection>
									<blocks>
										<m:HBox width="100%">
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Supplier:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input id="SupplierID" width="100%" value="" editable="false"/>
												<m:Label text="Supplier Name:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierName}" editable="false"/>
												<m:Label text="Supplier Contact1:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierDetail/SupplierContact1}" editable="false"/>
											</m:VBox>
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Inspection Conducted By:" width="100%"  class="sapUiSmallMarginTop"/>
												<!--<m:Input width="100%" value="{ZSQRMBWA>InspectionByName}"  editable="false"/>-->
												<m:Input width="100%" id="InspectionBy" value=""  editable="true"/>
												<m:Label text="Supplier Category:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierDetail/SupplierCategory}" editable="false"/>
												<m:Label text="Supplier Contact2:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierDetail/SupplierContact2}" editable="false"/>
											</m:VBox>
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Date of Inspection:" width="100%" class="sapUiSmallMarginTop"/>
												<m:DatePicker id="InspectionDate" width="100%" dateValue="{DateModel>/currentMonth}" displayFormat="short" editable="false"/>
												<m:Label text="Commodity:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierDetail/Commodity}" editable="false"/>
											</m:VBox>
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Other Contacts:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input id="OtherContacts" width="100%" value="{ZSQRMBWA>OtherContacts}"/>
												<m:Label text="Country:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="{ZSQRMBWA>SupplierDetail/Country}" editable="false"/>
											</m:VBox>
										</m:HBox>
									</blocks>
								</ObjectPageSubSection>
							</subSections>
						</ObjectPageSection>
						<ObjectPageSection title="Findings">
							<subSections>
								<ObjectPageSubSection>
									<blocks>
										<!--<m:VBox>-->
										<!--	<m:HBox>-->
												<m:Table width="100%" id="addInspectionTable" backgroundDesign="Solid" inset="false" items="{ path: '/' }" mode="None">
													<m:headerToolbar>
														<m:OverflowToolbar>
															<!--<Title text="Products" level="H2"/>-->
															<m:ToolbarSpacer/>
															<!-- Add the Personalization button -->
															<!--<CheckBox text="Enable Personalization Grouping" select="onTableGrouping"/>-->
															<!-- Add the Personalization button -->
															<m:Button text="Add Finding" type="Transparent" press="onDialogPress" icon="sap-icon://add" id="oButtonAddFinding"/>
															<m:Button icon="sap-icon://action-settings" press="onPersoButtonPressed">
																<m:layoutData>
																	<m:OverflowToolbarLayoutData priority="NeverOverflow"/>
																</m:layoutData>
															</m:Button>
														</m:OverflowToolbar>
													</m:headerToolbar>
													<m:columns>
														<m:Column  width="12em">
															<m:Text text="Finding id"/>
														</m:Column>
														<m:Column  width="12em">
															<m:Text text="Subject"/>
														</m:Column>
														<m:Column >
															<m:Text  text="Category"/>
														</m:Column>
														<m:Column  minScreenWidth="Desktop">
															<m:Text text="Question"/>
														</m:Column>
														<m:Column  minScreenWidth="Desktop">
															<m:Text text="Score"/>
														</m:Column>
														<m:Column  hAlign="End">
															<m:Text text="Status"/>
														</m:Column>
														<m:Column  hAlign="End">
															<m:Text text="Findings"/>
														</m:Column>
														<m:Column  hAlign="End">
															<m:Text text="Inspection Location"/>
														</m:Column>
														<m:Column  hAlign="End">
															<m:Text text="Edit"/>
														</m:Column>
														<m:Column  hAlign="End">
															<m:Text text="Delete"/>
														</m:Column>
													</m:columns>
													<m:items>
														<m:ColumnListItem>
															<m:cells>
																<m:Text text="{Id}"/>
																<m:Text text="{subject}"/>
																<m:Text text="{category}"/>
																<m:Text text="{question}"/>
																<m:Text text="{Score}"/>
																<m:Text text="{Status}"/>
																<m:Text text="{findings}"/>
																<m:Text text="{location}"/>
																<m:Button icon="sap-icon://edit" tooltip="Edit" press="onTableEditPress"/>
																<m:Button icon="sap-icon://delete" tooltip="Delete" press="onDeletePress"/>
															</m:cells>
														</m:ColumnListItem>
													</m:items>
												</m:Table>
										<!--	</m:HBox>-->
										<!--</m:VBox>-->
									</blocks>
								</ObjectPageSubSection>
							</subSections>
						</ObjectPageSection>
					</sections>
				</ObjectPageLayout>
				<m:footer>
					<m:Toolbar>
						<m:ToolbarSpacer/>
						<m:Button type="Accept" text="Save" press="onSaveInspectionPress"/>
						<m:Button type="Reject" text="Cancel" press="onAllInspectionPress"/>
					</m:Toolbar>
				</m:footer>
				<!--</content>-->
			</m:Page>
		</m:pages>
	</m:App>
</mvc:View>