<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns="sap.uxap" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:table="sap.ui.table"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" controllerName="com.sapZSQRMBWA.controller.NewInspection">
	<m:App id="newinspection">
		<m:pages>
			<m:Page title="SQRMBWA" showNavButton="true" navButtonPress="onNavBack">
				<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
					<headerTitle>
						<ObjectPageHeader objectTitle="Inspection"></ObjectPageHeader>
					</headerTitle>
					<headerContent>
						<m:ObjectAttribute title="Supplier Code" text="Hudson"/>
						<m:ObjectAttribute title="Supplier" text="1001553"/>
						<m:ObjectAttribute title="Supplier Category" text="ELE"/>
						<m:ObjectAttribute title="Commodity" text="Primary"/>
						<m:ObjectAttribute title="Country" text="USA"/>
						<m:ObjectAttribute title="Supplier Contact1" text="123 S Clark Street"/>
						<m:ObjectAttribute title="Supplier Contact2" text="Chicago"/>
					</headerContent>
					<sections>
						<ObjectPageSection title="Info">
							<subSections>
								<ObjectPageSubSection>
									<blocks>
										<m:HBox width="100%">
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Inspection Conducted By:" width="100%" id="__label0" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="1001553" id="__input0"/>
												<m:Label text="Supplier:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="SAP SE"/>
											</m:VBox>
											<m:VBox class="sapUiSmallMarginBegin" width="25%">
												<m:Label text="Date of Inspection:" width="100%" class="sapUiSmallMarginTop"/>
												<m:DatePicker width="100%" displayFormat="DD-MM-YYYY" value="27-10-2017"/>
												<m:Label text="Other Contacts:" width="100%" class="sapUiSmallMarginTop"/>
												<m:Input width="100%" value="SAP AG"/>
											</m:VBox>
										</m:HBox>
									</blocks>
								</ObjectPageSubSection>
							</subSections>
						</ObjectPageSection>
						<ObjectPageSection title="Findings">
							<subSections>
								<ObjectPageSubSection title=" ">
									<blocks>
										<m:VBox>
											<m:HBox>
												<m:Button text="Add Finding" type="Transparent" press="onDialogPress" icon="sap-icon://add" id="oButtonAddFinding"/>
											</m:HBox>
											<m:HBox>
												<m:Table width="100%" id="addInspectionTable" backgroundDesign="Solid" inset="false" items="{ path: '/', sorter: { path: 'Name' } }"
													mode="SingleSelectMaster" selectionChange="onSelectionChange">
													<m:columns>
														<m:Column width="12em">
															<m:Text text="Finding id"/>
														</m:Column>
														<m:Column width="12em">
															<m:Text text="Subject"/>
														</m:Column>
														<m:Column >
															<m:Text text="Category"/>
														</m:Column>
														<m:Column minScreenWidth="Desktop">
															<m:Text text="Question"/>
														</m:Column>
														<m:Column minScreenWidth="Desktop">
															<m:Text text="Score"/>
														</m:Column>
														<m:Column hAlign="End">
															<m:Text text="Status"/>
														</m:Column>
														<m:Column hAlign="End">
															<m:Text text="Findings"/>
														</m:Column>
														<m:Column hAlign="End">
															<m:Text text="Inspection Location"/>
														</m:Column>
														<m:Column hAlign="End">
															<m:Text text="Edit"/>
														</m:Column>
														<m:Column hAlign="End">
															<m:Text text="Delete"/>
														</m:Column>
													</m:columns>
													<m:items>
														<m:ColumnListItem>
															<m:cells>
																<m:Text text="{finding_id}"/>
																<m:Text text="{subject_id}"/>
																<m:Text text="{category_id}"/>
																<m:Text text="{question_id}"/>
																<m:Text text="{Score}"/>
																<m:Text text="{Status}"/>
																<m:Text text="{findings}"/>
																<m:Text text="{location}"/>
																<m:Button icon="sap-icon://edit" press="onTableEditPress"/>
																<m:Button icon="sap-icon://delete" press="onDeletePress"/>
															</m:cells>
														</m:ColumnListItem>
													</m:items>
												</m:Table>
											</m:HBox>
										</m:VBox>
									</blocks>
								</ObjectPageSubSection>
							</subSections>
						</ObjectPageSection>
					</sections>
				</ObjectPageLayout>
